<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="48dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#212121"
    >

    <ImageView
        android:id="@+id/timeline"
        android:layout_width="64dp"
        android:layout_height="48dp"
        android:src="@drawable/ic_timeline"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/market"
        />

    <ImageView
        android:id="@+id/market"
        android:layout_width="64dp"
        android:layout_height="48dp"
        android:src="@drawable/ic_market"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/timeline"
        app:layout_constraintEnd_toStartOf="@id/fares"
        />

    <ImageView
        android:id="@+id/fares"
        android:layout_width="64dp"
        android:layout_height="48dp"
        android:src="@drawable/ic_fares"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/market"
        app:layout_constraintEnd_toEndOf="parent"
        />

</androidx.constraintlayout.widget.ConstraintLayout>